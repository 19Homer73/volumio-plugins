{
  "page": {
    "label": "Volumio Spotify connect Configuration"
  },
  "sections": [ 
   {
      "id": "section_account",
      "element": "section",
      "label": "Spotify account",
      "icon": "fa-plug",
      "onSave": {"type":"controller", "endpoint":"music_service/volspotconnect", "method":"saveVolspotconnectAccount"},
      "saveButton": {
        "label": "Save",
        "data": [
          "username",
          "password",
          "bitrate",
	  "devicename"
        ]
      },
      "content": [
        {
          "id": "username",
          "type":"text",
          "element": "input",
          "doc": "This is the username of your Spotify account",
          "label": "Username",
          "value": ""
        },
        {
          "id": "password",
          "type":"password",
          "element": "input",
          "doc": "This is the password of your Spotify account",
          "label": "Password",
          "value": ""
        },
{
          "id":"bitrate",
          "element": "switch",
          "doc": "High bitrate",
          "label": "Set for high bitrate",
          "value": true
        },
       	 {
          "id":"devicename",
	"type":"text",
          "element": "input",
          "doc": "name of device",
          "label": "name of device",
          "value": "volumio2"
        }
      ]
   }
  ]
}